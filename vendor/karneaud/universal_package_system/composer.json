{
    "name": "karneaud/universal_package_system",
    "description": "PHP SDK for interacting with RESTful API from Universal Package System",
    "type": "library",
    "license": "MIT",
    "homepage": "https://universalpackagesys.com",
    "authors": [
        {
            "name": "karneaud",
            "email": "kendall.arneaud@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.0.0 <7.5.0",
        "psr/http-message": "^1.0",
        "php-http/client-implementation": "^1.0",
        "php-http/httplug": "^1.0",
        "php-http/message-factory": "^1.0",
        "php-http/discovery": "^1.0",
        "nesbot/carbon": "^2.24.0"
    },
    "require-dev": {
        "kahlan/kahlan": "*",
        "php-http/guzzle6-adapter":"*",
        "php-http/mock-client": "^1.0",
        "php-http/message": "^1.0",
        "guzzlehttp/psr7": "^1.0",
        "fzaninotto/faker": "^1.9@dev"
    },
    "autoload": {
        "psr-4": {
            "UniversalPackageSystems\\": "src/",
            "UniversalPackageSystems\\Endpoint\\":"src/endpoints/"
        }
    }
}
